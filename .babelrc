{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": ["transform-vue-jsx", "transform-runtime", ["component", [
    {
      "libraryName": "vue-zyby-ui",
      "camel2Dash": false,
      "styleLibrary": {
        "name": "theme",
        "base": true
      }
    },
    {
      "libraryName": "mint-ui",
      "style": true
    }
  ]]]
}
